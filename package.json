{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "npm-run-all --parallel typings startMessage",
    "start": "npm-run-all --parallel security-check open:src",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "open:src": "ts-node buildScripts/srcServer.ts",
    "share": "npm-run-all --parallel open:src localtunnel",
    "typings": "typings install",
    "startMessage": "ts-node buildScripts/startMessage.ts"
  },
  "author": "Jan Van Braeckel",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "0.13.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "typescript": "^2.3.2",
    "typings": "^2.1.1",
    "webpack": "2.4.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
