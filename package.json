{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "ts-node buildScripts/startMessage.ts",
    "start": "npm-run-all --parallel security-check open:src test",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "open:src": "ts-node buildScripts/srcServer.ts",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter spec buildScripts/testSetup.js -r ts-node/register src/**/*.test.ts"
  },
  "author": "Jan Van Braeckel",
  "dependencies": {
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/chai": "3.5.2",
    "@types/mocha": "2.2.41",
    "@types/node": "7.0.18",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "10.1.0",
    "json-schema-faker": "0.4.1",
    "json-server": "0.10.0",
    "localtunnel": "1.8.2",
    "mocha": "3.3.0",
    "nock": "9.0.13",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.17.0",
    "ts-loader": "2.0.3",
    "ts-node": "3.0.2",
    "tslint": "5.2.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.3.2",
    "webpack": "2.5.0",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
