    "compression": "1.6.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.1.0",
    "json-schema-faker": "^0.4.1",
    "json-server": "^0.10.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.3.0",
    "nock": "^9.0.13",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
